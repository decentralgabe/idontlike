{
  "name": "idontlike",
  "version": "0.1",
  "description": "sometimes the best choice is no choice",
  "background": {
    "scripts": ["like.js"],
    "run_at": "document_idle"
  },
  "page_action" :
  {
    "default_icon" : "like.png",
    "default_title" : "idontlike"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "https://www.facebook.com/*",
    "http://www.facebook.com/*"
  ],
  "icons" : {
    "48" : "like-48.png",
    "128" : "like-128.png"
  },
  "manifest_version": 2
}

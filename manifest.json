{
  "name": "idontlike",
  "version": "1.0",
  "description": "sometimes the best choice is no choice",
  "background": {
    "page": "like.html"
  },
  "page_action" :
  {
    "default_icon" : "like.png",
    "default_title" : "idontlike"
  },
  "permissions": [
    "tabs",
    "https://www.facebook.com/*", 
    "http://www.facebook.com/*"
  ],
  "content_scripts": [
    {
     "matches": ["https://www.facebook.com/*", 
     "http://www.facebook.com/*"],
     "js": ["like.js"]
    }
  ],
  "manifest_version": 2
}

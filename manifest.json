{
  "manifest_version": 2,

  "name": "idontlike",
  "description": "sometimes the best choice is no choice",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "contextMenus",
    "tabs",
    "storage",
    "*://*.facebook.com/*"
  ]
}
